(let
  (factorial
    (\ (n)
      (cond 
        (n 
          {n*(factorial {n-1})}
        )
        (1 1)
      )
    )
  )
  (cnk (\ (n k)
      (cond 
        ((< n 0) 0)
        ((< k 0) 0)
        ((< n k) 0)
        ((and (= n 0) (= k 0)) 1)
        (1 (+ (cnk {n-1} {k-1}) (cnk {n-1} {k})))
      )
    ) 
  )
)
